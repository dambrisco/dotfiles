" Commands
" ------------------------------------------------------------

" easily get to vim config
command Vimrc        :e ~/.vimrc
command VimCommands  :e ~/.vim/vimrc_commands
command VimMaps      :e ~/.vim/vimrc_mappings
command VimPlugins   :e ~/.vim/vimrc_plugins
command VimSettings  :e ~/.vim/vimrc_settings
command VimBundles   :e ~/dotfiles/vim_bundles
command VimNT :NERDTree ~/.vim

" easily get to bashrc
command Bashrc :e ~/.bashrc
