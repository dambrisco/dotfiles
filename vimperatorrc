"2.3.1 (created: 2010/04/07 18:56:53)

" Makes cmd-l use vimperator instead of FF location bar
map <M-l> :o<Space>

" Makes cmd-f use vimperator instead of FF search
map <M-f> /

" Tab opening with vimperator
map <M-t> t

" Makes autocompletion show up automatically
set wildoptions=auto

" Gets rid of annoying visual bell when using firefox builtin keyboard shortcuts
set visualbell
:hi Bell display: none;

set runtimepath=/Users/jgarvin/.vimperator
source! /Users/jgarvin/.vimperatorrc.local

" vim: set ft=vimperator:
